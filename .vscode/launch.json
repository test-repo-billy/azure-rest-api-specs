  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha Tests",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "--timeout",
        "999999",
        "--colors",
        "${workspaceFolder}/test/syntax.js"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "spec-gen-run",
      "program": "${workspaceFolder}/eng/scripts/spec-gen-sdk.js",
      "args": [
        "--scp", ".",
        "--sdp", "/home/raychen/repos/github/azure-sdk-for-net",
        "--l", "azure-sdk-for-net",
        "--wk", "../"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${file}"
    }
  ]
}
